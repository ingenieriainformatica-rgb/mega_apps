<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-extend="OrderReceipt" t-inherit="point_of_sale.OrderReceipt" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('pos-receipt-taxes')]" position="before">
            <t t-if="env.services.pos.config.sh_enable_rounding">
                <div>Rounding<span t-esc="props.data.rounding_amount" class="pos-receipt-right-align" /></div>
            </t>
        </xpath>
        <xpath expr="//Orderline" position="attributes">
            <attribute name="t-if">!line.sh_hide_orderline</attribute>
           
        </xpath>
    </t>

</templates>