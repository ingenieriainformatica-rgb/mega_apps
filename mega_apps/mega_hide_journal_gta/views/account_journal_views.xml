<odoo>

    <record id="view_account_journal_form_inherit_cash_transfer_journal" model="ir.ui.view">
        <field name="name">account.journal.form.inherit.cash.transfer</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <!-- Insertamos en el notebook -->
            <xpath expr="//notebook" position="inside">
                <page string="Visible Diario?">
                    <group>
                        <field name="visible_journal" string="No Visible?"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
